#include <iostream>
using namespace std;


// for templates, define all the definition and actual implementation
// in the header files

// after defining the class T, T is now a generic type that we can use
template<class T>
class Test {

};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
