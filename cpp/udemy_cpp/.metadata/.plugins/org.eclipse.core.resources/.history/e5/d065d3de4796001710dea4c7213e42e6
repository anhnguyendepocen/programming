#include <iostream>
#include <vector>
using namespace std;

int findKthLargest(vector<int>& nums, int k) {
	sort(nums.begin(), nums.end());
	return nums[k - 1];
}

int main() {
	vector<int> nums = {3, 2, 1, 5, 6, 4};
	cout << findKthLargest(nums, 2) << endl;
	return 0;
}
