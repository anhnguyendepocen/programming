#ifndef COMPLEX_H_
#define COMPLEX_H_

namespace basics {

class Complex {
private:
	double real;
	double imaginary;

public:
	Complex();
	Complex(double real, double imaginary);
	Complex(const Complex &other);
	const Complex &operator=(const Complex &other);
	virtual ~Complex();
};

} /* namespace basics */

#endif /* COMPLEX_H_ */
