#include <iostream>
#include <vector>
using namespace std;

int findKthLargest(vector<int>& nums, int k) {
	sort(nums.begin(), nums.end());
	return nums[k - 1];
}

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
